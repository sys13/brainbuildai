{
	"name": "brainbuildai",
	"version": "0.0.0",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"imports": {
		"#*": "./*"
	},
	"scripts": {
		"build": "run-s build:*",
		"build:rr": "react-router build",
		"build:server": "tsx ./other/build-server.ts",
		"coverage": "vitest run --coverage",
		"db:gen:vectors": "tsx genVectors.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "tsx ./app/utils/migrate.server.ts",
		"db:proxy": "fly proxy 15432:5432 -a brainbuildaidb",
		"db:reseed": "sh ./other/db-reseed.sh",
		"db:reset": "sh ./other/db-reset.sh",
		"db:seed": "tsx -r dotenv/config ./drizzle/runSeed.ts",
		"db:studio": "drizzle-kit studio",
		"dev": "cross-env NODE_ENV=development MOCKS=true node ./server/dev-server.js",
		"dev:no-mocks": "cross-env NODE_ENV=development node ./server/dev-server.js",
		"knip": "knip",
		"lint": "run-s lint:*",
		"lint:fix": "biome check --write --unsafe",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\"",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"pretest:e2e:run": "npm run build",
		"test:e2e:run": "cross-env CI=true playwright test",
		"setup": "npm run build && playwright install",
		"start": "cross-env NODE_ENV=production node .",
		"start:mocks": "cross-env NODE_ENV=production MOCKS=true tsx .",
		"test": "vitest",
		"test:e2e": "npm run test:e2e:dev --silent",
		"test:e2e:dev": "playwright test --ui",
		"test:e2e:install": "npx playwright install --with-deps chromium",
		"typecheck": "react-router typegen && tsc",
		"validate": "run-p \"test -- --run\" lint typecheck test:e2e:run db:seed"
	},
